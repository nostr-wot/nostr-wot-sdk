{
  "name": "nostr-wot-sdk",
  "version": "0.2.0",
  "description": "JavaScript/TypeScript SDK for querying Nostr Web of Trust",
  "author": "Leon Acosta <leon@nostr-wot.com>",
  "license": "MIT",
  "keywords": [
    "nostr",
    "web-of-trust",
    "wot",
    "social-graph",
    "trust",
    "decentralized"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nostr-wot/nostr-wot-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/nostr-wot/nostr-wot-sdk/issues"
  },
  "homepage": "https://github.com/nostr-wot/nostr-wot-sdk#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./local": {
      "import": {
        "types": "./dist/local/index.d.ts",
        "default": "./dist/local/index.js"
      },
      "require": {
        "types": "./dist/local/index.d.cts",
        "default": "./dist/local/index.cjs"
      }
    },
    "./react": {
      "import": {
        "types": "./dist/react/index.d.ts",
        "default": "./dist/react/index.js"
      },
      "require": {
        "types": "./dist/react/index.d.cts",
        "default": "./dist/react/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.0",
    "eslint": "^8.55.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "sideEffects": false
}
